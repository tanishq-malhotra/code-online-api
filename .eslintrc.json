{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": "off",
        "new-cap":"off",
        "require-jsdoc":"off",
        "no-unused-vars":"off",
        "camelcase":"off"
    }
}